{
  "clean_urls": true,
  "https_only": true,
  "root": "_site/",
  "headers": {
    "/": {
      "Cache-Control": "no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "**.ttf": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "**.png": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    }
  }
}
